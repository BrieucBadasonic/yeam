<%= simple_form_for @benevole do |f| %>
  <%= f.input :first_name, placeholder: "Prenom", label: false %>
  <%= f.input :last_name, placeholder: "Nom de famille", label: false %>
  <%= f.input :email, placeholder: "Email", label: false %>
  <%= f.input :telephone, placeholder: "Telephone", label: false %>
  <%= f.submit class:"btn btn-warning"%>
<% end %>
